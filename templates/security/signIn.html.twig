
{% block body %}

		<form action='{{path('signIn')}}' method='post'>
			<div class='form-group'>
				<input placeholder='Email...' required name='_username' type='text' class='form-control'>
			</div>
			<div class='form-group'>
				<input placeholder='Password...' required name='_password' type='password' class='form-control'>
			</div>
			<div class='form-group'>
				<button type='submit' class='btn btn-primary dropdown-toggle get-started-btn mt-1 mb-1'>Connexion</button>
			</div>
			
		</form>
{% endblock %}
